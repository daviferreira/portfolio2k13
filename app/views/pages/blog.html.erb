<div class="posts-container posts-page secondary-page">
    <div class="container clearfix">
        <section class="nav-sidebar">
            <header class="clearfix">
                <h1>Blog</h1>
                <h2>showing all</h2>
            </header>
        </section>
        <section class="posts clearfix main-content">
            <% @posts.each do |post| %>
                <article>
                    <h1><%= post.title %></h1>
                </article>
            <% end %>
        </section>
    </div>
</div>